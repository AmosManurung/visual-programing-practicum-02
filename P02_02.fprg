<flowgorithm fileversion="3.0">
    <attributes>
        <attribute name="name" value=""/>
        <attribute name="authors" value="amos_"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2023-10-01 01:50:26 PM"/>
