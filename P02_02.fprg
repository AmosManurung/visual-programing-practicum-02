        <attribute name="edited" value="YW1vc187TEFQVE9QLUc0NlNJUktEOzIwMjMtMTAtMDE7MDE6NTA6MjYgUE07MzsyOTcx"/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="namagudang, AC, lantai, ketersediaanteknolgi" type="String" array="False" size=""/>
            <declare name="kapasitasgudang, jumlahbukusaatini, skor" type="Integer" array="False" size=""/>
            <input variable="namagudang"/>
            <input variable="AC"/>
            <input variable="lantai"/>
            <input variable="ketersediaanteknologi"/>
            <input variable="kapasitasgudang"/>
            <input variable="jumlahbukusaatini"/>
            <input variable="skor"/>
            <assign variable="skor" expression="(jumlahbukusaatini/kapasitasgudang)"/>
            <if expression="skor &gt;=40">
                <then>
                    <assign variable="kualitasgudang" expression="gudangelit"/>
                </then>
                <else>
                    <if expression="skor &gt;=25">
                        <then>
                            <assign variable="kualitasgudang" expression="gudangstandar"/>
                        </then>
                        <else>
                            <if expression="skor &lt;25">
                                <then>
                                    <assign variable="kualitasgudang" expression="gudangperlupeningkatan"/>
                                </then>
                                <else/>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="&quot;informasi gudang&quot;" newline="True"/>
            <output expression="namagudang &amp;&quot;|&quot;&amp; AC&amp;&quot;|&quot;&amp;lantai&amp;&quot;|&quot;&amp;ketersediaanteknolgi&amp;&quot;|&quot;&amp;kapasitasgudang&amp;&quot;|&quot;&amp; jumlahbukusaatini&amp;&quot;|&quot;&amp;skor" newline="True"/>
        </body>
    </function>
</flowgorithm>
